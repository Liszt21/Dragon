{
    "homepage": "https://github.com/pyenv-win/pyenv-win",
    "description": "pyenv for Windows",
    "version": "1.2.4",
    "url": "https://github.com/pyenv-win/pyenv-win/archive/v1.2.4.zip",
    "bin": "pyenv-win\\bin\\pyenv",
    "extract_dir": "pyenv-win-1.2.4",
    "env_add_path": ["pyenv-win\\bin","pyenv-win\\shims"],
    "env_set": {
        "PYENV_HOME": "$dir\\pyenv-win"
